{
  "name": "vintage-vault-web",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run typecheck && vite build",
    "dev": "vite",
  "api:types": "node ../../backend/scripts/generate_typescript_client.py || python ../../backend/scripts/generate_typescript_client.py",
    "format": "prettier --cache --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "start": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@effect-ts/core": "latest",
    "@tsplus/stdlib": "latest",
    "@vitejs/plugin-react": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-dom": "^1.52.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.52.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.3",
    "typescript": "^5.0.0",
    "vite": "7.1.1"
  }
}
