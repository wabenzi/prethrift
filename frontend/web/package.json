{
  "name": "vintage-vault-web",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run typecheck && vite build",
    "dev": "vite",
    "api:types": "node ../../backend/scripts/generate_typescript_client.py || python ../../backend/scripts/generate_typescript_client.py",
    "demo": "node demo_frontend_integration.js",
    "demo:text": "node demo_frontend_integration.js --test-case=text",
    "demo:visual": "node demo_frontend_integration.js --test-case=visual",
    "demo:hybrid": "node demo_frontend_integration.js --test-case=hybrid",
    "demo:all": "node demo_frontend_integration.js --test-case=all --export-results",
    "format": "prettier --cache --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "start": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@effect-ts/core": "latest",
    "@tsplus/stdlib": "latest",
    "@vitejs/plugin-react": "^5.0.0",
    "axios": "^1.11.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-dom": "^1.52.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.52.3",
    "husky": "9.0.11",
    "jsdom": "^24.1.3",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.3",
    "typescript": "^5.0.0",
    "vite": "7.1.1",
    "vitest": "^3.2.4"
  }
}
